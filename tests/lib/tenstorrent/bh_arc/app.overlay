/ {
	fwtable: fwtable {
		compatible = "tenstorrent,bh-fwtable";
		status = "okay";
		flash-dev = <&flashcontroller0>;
	};

	pll0: pll0
	{
		compatible = "tenstorrent,clock-control-emul";
		status = "okay";
	};

	pll4: pll4
	{
		compatible = "tenstorrent,clock-control-emul";
		status = "okay";
	};

	dma1: noc_dma {
		status = "okay";
	};

	pvt: pvt@80030700 {
		compatible = "tenstorrent,bh-pvt";
		reg = <0x80030700 0x4>;
		num_pd = <16>;
		num_vm = <8>;
		num_ts = <8>;
		status = "okay";
	};
};

&i2c0 {
	smbus_target0: smbus@0a {
		status = "okay";
		compatible = "zephyr,smbus_target";
		reg = <0x0a>;
	};
};
